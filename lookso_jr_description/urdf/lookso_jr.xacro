<?xml version="1.0"?>
<!-- Open_Manipulator Chain -->
<robot name="gazebo_lookso_jr" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- some parameters -->
  <xacro:property name="pi" value="3.141592654" />

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find lookso_jr_description)/urdf/lookso_jr.gazebo.xacro" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find lookso_jr_description)/urdf/materials.xacro" />

  <!-- World -->
  <link name="world">
  </link>

  <!-- World fixed joint-->
  <joint name="world_fixed" type="floating">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="link1"/>
  </joint>

  <!-- Link 1 -->
  <link name="link1">
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="${0.1*100}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
               iyy="1.0" iyz="0.0"
               izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0.0 0.0 0.054" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1"/>
  </joint>

  <!-- Link 2 -->
  <link name="link2">
    <collision>
      <origin xyz="0.0 0.0 -0.054" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0.0 0.0 -0.054" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.4" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
               iyy="1.0" iyz="0.0"
               izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 2 -->
  <joint name="joint2" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="-0.0263 0.0 0.0658" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1"/>
  </joint>

  <!-- Link 3 -->
  <link name="link3">
    <collision>
      <origin xyz="0.0263 0.0 -0.1198" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0.0263 0.0 -0.1198" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="${0.05*100}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
               iyy="1.0" iyz="0.0"
               izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 3 -->
  <joint name="joint3" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0.0 0 0.075" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1"/>
  </joint>

  <!-- Link 4 -->
  <link name="link4">
    <collision>
      <origin xyz="0.0263 0.0 -0.1948" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link4.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0.0263 0.0 -0.1948" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link4.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="${0.2*100}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
               iyy="1.0" iyz="0.0"
               izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 4 -->
  <joint name="joint4" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0.012 0.0 0.0373" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1"/>
  </joint>

  <link name="link5">
    <collision>
      <origin xyz="0.0143 0.0 -0.2321" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0.0143 0.0 -0.2321" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://lookso_jr_description/meshes/link5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0.075 0 0.050" />
      <mass value="${0.7*100}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
               iyy="1.0" iyz="0.0"
               izz="1.0" />
    </inertial>
  </link>
</robot>
